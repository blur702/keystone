REFRESH_TOKEN_EXPIRES_IN=7d
BCRYPT_ROUNDS=10
SESSION_TIMEOUT=86400000

# CORS
CORS_ORIGIN=http://localhost

# API URL for frontend
VITE_API_URL=http://localhost/api

# Logging
LOG_LEVEL=info