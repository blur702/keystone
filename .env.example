# Environment Configuration
NODE_ENV=production

# Database
DB_HOST=postgres
DB_PORT=5432
DB_NAME=keystone
DB_USER=keystone
DB_PASSWORD=change-this-password

# JWT Authentication
JWT_SECRET=change-this-secret-in-production
